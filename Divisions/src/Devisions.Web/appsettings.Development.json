{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5434;Database=devisions_service_db;Username=postgres;Password=postgres;"
  },
  "AllowedHosts": "*",
  "Cleaner": {
    "RunTime": "23:00:00"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  }
}